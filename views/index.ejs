	</section>

	<section class="bodasContainer">
		<img src="/pool party/pool-01.jpg">
		<img src="/pool party/pool-02.jpg">
	</section>

	<section class="iluminacion">
		<h2>CONCIERTOS</h2>
		<a href='#' class='b' data-info='conciertos' title='¡¡Haz click aqui!!'>Ver mas</a>
	</section>

	<section class="bodasContainer">
		<img src="/conciertos/conciertos-01.JPG">
		<img src="/conciertos/conciertos-02.PNG">
